        <name>Generate Build Test Data Completed</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading || newValue === '') {
      return;
	}
	
	var fieldName1 = 'build_test_data_completed';
	var fieldName2 = 'build_test_data_completed_at';
	//Type appropriate comment here, and begin script below
	if (newValue == 'true') {  //newValue is a string so needs to be 'true' not true
		g_form.getElement(fieldName1).style.backgroundColor = "lightgreen";			
		g_form.getElement(fieldName2).style.backgroundColor = "lightgreen";	
   
		// Set the field labels to be green
		// Need to set flsh twice in order to get it to stick
		// Do this before setting the fields readonly
		g_form.flash(fieldName1, "lightgreen", 0);
		g_form.flash(fieldName1, "lightgreen", 0);
   
		g_form.flash(fieldName2, "lightgreen", 0);
		g_form.flash(fieldName2, "lightgreen", 0);	   
	}
}]]></script>
